

<article class="annonces_index">
    <section class="list_annonces">
        <h1>Annonces immobilieres</h1>
        <ul class="filtre_tags">
            <li class="filtre_tag">Maison</li>
            <li class="filtre_tag">Appartement</li>
        </ul>
        <ul></ul>
        <ul class="new_annonces">
            <% 10.times do %>
            <%= render "achats/annonce" %>
            <% end %>
        </ul>
        <div>
            <ul>
                <li>Alsace</li>
                <li>Bas-Rhin</li>
                <li>Haut-Rhin</li>
                <li>Strasbourg</li>
            </ul>
        </div>
    </section>
    <section class="carte_fixed">
        <div id="map"
            data-controller="map"
            data-map-markers-value="<%= @markers.to_json %>"
            data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>">
        </div>
    </section> 
</article>
    